
<p id="notice"><%= notice %></p>

<h1>Listing Origins</h1>

<table>
  <thead>
    <tr>
      <th><%= link_to 'Colector', origins_path(order: 'colector') %></th>
      <th>Address</th>
      <th>Feeding</th>
      <th>Capmethod</th>
      <th>Features</th>
      <th>Province</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origins.each do |origin| %>
      <tr>
        <td><%= origin.colector %></td>
        <td><%= origin.address %></td>
        <td><%= origin.feeding %></td>
        <td><%= origin.capmethod %></td>
        <td><%= origin.features %></td>
        <td><% if origin.province %>
        <%= origin.province.name %>
        <% end %></td>
        
        <td><%= link_to 'Show', origin %></td>
        <td><%= link_to 'Edit', edit_origin_path(origin) %></td>
        <td><%= link_to 'Destroy', origin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Origin', new_origin_path %>
<%= link_to 'Volver', entries_path %>
